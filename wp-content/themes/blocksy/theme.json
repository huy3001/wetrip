{
	"version": 2,

	"settings": {
		"appearanceTools": true,

		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": ["px", "em", "rem", "%", "vh", "vw"]
		},

		"layout": {
			"contentSize": "var(--block-max-width)",
